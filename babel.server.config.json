{
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "edge": "17",
            "firefox": "60",
            "chrome": "67",
            "safari": "11.1"
          },
          "modules": false,
          "useBuiltIns": "entry",
          "corejs": "3.10.0"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-optional-chaining",
      "@babel/plugin-syntax-dynamic-import"
    ]
  }
  